<svg width="300" height="200" viewBox="0 0 300 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="200" fill="#f8fafc"/>
  
  <!-- Garage/Shop Background -->
  <rect x="20" y="40" width="260" height="120" fill="#e2e8f0" stroke="#cbd5e1" stroke-width="2"/>
  
  <!-- Car -->
  <g transform="translate(80, 80)">
    <!-- Car Body -->
    <rect x="0" y="20" width="140" height="40" rx="8" fill="#3b82f6"/>
    <!-- Windshield -->
    <rect x="10" y="15" width="60" height="25" rx="4" fill="#bfdbfe"/>
    <rect x="70" y="15" width="60" height="25" rx="4" fill="#bfdbfe"/>
    <!-- Wheels -->
    <circle cx="25" cy="70" r="12" fill="#1f2937"/>
    <circle cx="25" cy="70" r="8" fill="#6b7280"/>
    <circle cx="115" cy="70" r="12" fill="#1f2937"/>
    <circle cx="115" cy="70" r="8" fill="#6b7280"/>
  </g>
  
  <!-- Repair Tools -->
  <g transform="translate(40, 100)">
    <!-- Wrench -->
    <rect x="0" y="0" width="20" height="4" rx="2" fill="#ef4444"/>
    <circle cx="2" cy="2" r="3" fill="#dc2626"/>
    <!-- Screwdriver -->
    <rect x="25" y="-2" width="3" height="20" fill="#fbbf24"/>
    <rect x="24" y="18" width="5" height="8" rx="1" fill="#f59e0b"/>
  </g>
  
  <!-- Service Text -->
  <text x="150" y="180" text-anchor="middle" fill="#1f2937" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Auto Repair</text>
</svg>